<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.shape.Rectangle?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
  xmlns="http://javafx.com/javafx/8"
  fx:controller="application.controller.GameController">
  <Pane id="base" layoutX="150.0" layoutY="50.0" prefHeight="300.0" prefWidth="300.0"
    fx:id="base_square">
    <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#f2f2f2" height="270.0" layoutX="15.0"
      layoutY="15.0"
      stroke="BLACK" strokeType="INSIDE" width="270.0" fx:id="game_panel"/>
    <Line endX="170.0" layoutX="115.0" layoutY="105.0" startX="-100.0"/>
    <Line endX="170.0" layoutX="115.0" layoutY="195.0" startX="-100.0"/>
    <Line endX="170.0" layoutX="70.0" layoutY="150.0" rotate="270.0" startX="-100.0"/>
    <Line endX="170.0" layoutX="160.0" layoutY="151.0" rotate="90.0" startX="-100.0"/>
  </Pane>
  <Button layoutX="50.0" layoutY="300.0" mnemonicParsing="false" onAction="#backButtonClick"
    text="Back" fx:id="backButton"/>
</AnchorPane>
