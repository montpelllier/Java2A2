<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity"
  minHeight="-Infinity"
  minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8"
  fx:controller="application.controller.HomeController" fx:id="home">
  <BorderPane prefHeight="640.0" prefWidth="590.0">
    <top>
      <Text BorderPane.alignment="CENTER" strokeType="OUTSIDE" strokeWidth="2.0" text="null"
        fx:id="info"/>
    </top>
  </BorderPane>
  <Button layoutX="240.0" layoutY="120.0" mnemonicParsing="false" onAction="#startButtonClick"
    text="Start Game" fx:id="startButton"/>
  <Button layoutX="240.0" layoutY="160.0" mnemonicParsing="false" onAction="#settingButtonClick"
    text="Setting" fx:id="settingButton"/>
  <Button layoutX="240.0" layoutY="200.0" mnemonicParsing="false" onAction="#logoutButtonClick"
    text="Logout" fx:id="logoutButton"/>
  <Button layoutX="240.0" layoutY="240.0" mnemonicParsing="false" onAction="#quitButtonClick"
    text="Quit" fx:id="quitButton"/>
</AnchorPane>