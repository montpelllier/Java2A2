<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity"
  minHeight="-Infinity"
  minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8"
  fx:controller="application.controller.LoginController" fx:id="login">
  <BorderPane prefHeight="20.0" prefWidth="600.0"/>
  <Button layoutX="140.0" layoutY="206.0" mnemonicParsing="false" onAction="#loginButtonClick"
    text="Login" fx:id="loginButton"/>
  <Button layoutX="140.0" layoutY="246.0" mnemonicParsing="false" onAction="#registerButtonClick"
    text="Register" fx:id="registerButton"/>
  <Label layoutX="99.0" layoutY="146.0" text="Password: "/>
  <Label layoutX="99.0" layoutY="96.0" text="User name: "/>
  <TextField layoutX="174.0" layoutY="141.0" promptText="password" fx:id="loginPassword"/>
  <TextField layoutX="174.0" layoutY="91.0" promptText="user name" fx:id="loginUsername"/>
</AnchorPane>